{
  "name": "hakushiki-code",
  "displayName": "HAKUSHIKI Code",
  "description": "AI generates code from your comments!",
  "version": "0.0.1",
  "publisher": "koshin",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.jpeg",
  "activationEvents": [
    "onCommand:hakushiki-code.activate"
  ],
  "main": "./extension.js",
  "bugs": {
    "url": "https://github.com/koshin01/hakushiki_code/issues",
    "email": "ekoshin2001@icloud.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koshin01/hakushiki_code"
  },
  "contributes": {
    "commands": [
      {
        "command": "hakushiki-code.activate",
        "title": "HAKUSHIKI Code Activate"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "hakushiki-code.activate",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.3"
  }
}
